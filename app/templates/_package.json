{
  "jspm": {
    "main": "app/main",
    "directories": {
      "lib": "src"
    },
    "dependencies": {
      "argon": "gatech:ael/argon.js@0.1.2",
      "image": "github:systemjs/plugin-image@^0.1.0",
      "three": "github:jiyinyiyong/three.js@^0.71.0",
      "threestrap": "github:unconed/threestrap@^0.0.10"
    },
    "overrides": {
      "github:jiyinyiyong/three.js@0.71.0": {
        "main": "three",
        "format": "global",
        "shim": {
          "three": {
            "exports": "THREE"
          }
        }
      },
      "github:unconed/threestrap@0.0.10": {
        "main": "threestrap",
        "format": "global",
        "directories": {
          "dist": "build"
        },
        "registry": "jspm",
        "dependencies": {
          "three": "github:jiyinyiyong/three.js@^0"
        },
        "shim": {
          "threestrap": {
            "deps": [
              "three"
            ],
            "exports": "THREE"
          }
        }
      }
    }
  },
  "devDependencies": {
    "cheerio": "^0.18.0",
    "express": "^4.11.1",
    "gulp": "^3.8.10",
    "socket.io": "1.3.2"
  }
}
